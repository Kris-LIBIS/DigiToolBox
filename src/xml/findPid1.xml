<xb:x_queries xmlns:xb="http://com/exlibris/digitool/repository/api/xmlbeans">
    <x_query type="xql" set_name="SET1">
        <x_select from_answer="0" to_answer="%max_result%">
            <element>pid</element>
        </x_select>
        <x_where>
            <x_condition>
                <type>exact</type>
                <element>label</element>
                <value>%label%</value>
            </x_condition>
        </x_where>
        <x_sort>
            <by>pid</by>
            <x_order>asc</x_order>
        </x_sort>
    </x_query>
</xb:x_queries>
